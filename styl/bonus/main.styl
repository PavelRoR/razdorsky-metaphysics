#main
	width: 100%
	background-color: #000
	background-image url(../img/main_bg.png)
	background-repeat no-repeat
	background-position center top
	background-size: cover
	overflow hidden
	padding-bottom:  60px
	position relative
	&:before
		content: '';
		display: block;
		position: absolute;
		bottom: 0;
		left: 0;
		right: 0;
		width: 100%;
		z-index: 15;
		background-image: url("../img/clouds_main.png");
		background-repeat: no-repeat;
		background-position: center top;
		background-size: cover;
		height: 291px;
		margin: 0 auto;
	.container
		position relative
#header
	padding: 31px 0
	overflow hidden
	z-index: 9
	position relative
	margin-bottom: 18px
#logo
	float: left
	opacity .5
	width 193px
	height auto
.contacts
	float: right
	text-align: right
	line-height  12px
	a
		font-size: 10px
		&:focus
			text-decoration: none
		.fa
			color: #538ffe
			font-size: 16px
			margin: 0 15px 0  40px
			line-height: 12px
		.fa+span
			line-height: 12px
			font-size: 16px;
			font-family: "OpenSansLight", sans-serif;
			color: rgba(#fff, .6)
			text-align: right;
			&:hover
				text-decoration underline
.tel:first-child
	.fa-phone
		margin-left: 0
.pre
	text-align: center
	font-family: "OpenSansBold", sans-serif
	color: rgb(224, 13, 2);
	text-transform: uppercase;
	letter-spacing: 1.3px;
.main_title
	text-align: center
	font-size: 47px;
	font-family: "AkrobatBlack", sans-serif
	color: rgb(254, 254, 255);
	text-transform: uppercase;
	line-height: 1.17;
	text-shadow: 3.064px 2.571px 0px rgba(13, 10, 10, 0.41);
	margin: 17px 0
	letter-spacing: 1.8px;
.main_sub_title
	text-align: center
	font-size: 20px;
	font-family: "OpenSansRegular",sans-serif
	color: rgb(255, 255, 255);
	line-height: 1.3;
	width 100%
.main_date
.remind
	text-align: center
	font-size: 28px;
	font-family: "AkrobatBlack", sans-serif
	color: rgb(255, 255, 255);
	text-transform: uppercase;
	margin 5px 0 21px
.remind
	font-size: 24px
	margin -10px 0 10px
.main_date_blue
	text-align: center
	color #538ffe
.main_date_time
	font-family: "AkrobatRegular", sans-serif
	letter-spacing: -0.2px;
.video_wrapper_main
.video_wrapper_ht
	max-width 560px
	z-index 25
.video_wrapper_ht
	margin-top: 20px
.flip-clock-wrapper
	width 100%
	max-width 360px
	margin: 20px auto
	font-size: 13px
.flip-clock-wrapper ul
	height 50px
	margin 3px
.flip-clock-wrapper ul li
	line-height: 50px
.flip-clock-wrapper ul li a div.up::after
	top 24px
.flip-clock-wrapper ul li a div div.inn
	font-size: 40px
	background-color: #538ffe
.flip-clock-divider
	height 50px
.flip-clock-dot
	background-color: #538ffe
	width 7px
	height 7px
	left 3px
.flip-clock-dot.top
	top 31px
.flip-clock-dot.bottom
	bottom: 27px
.flip-clock-divider .flip-clock-label
	color #538ffe
	right: -58px
.flip-clock-divider.hours .flip-clock-label
	right  -63px
.flip-clock-divider.minutes .flip-clock-label
.flip-clock-divider.seconds .flip-clock-label
	color #538ffe
	right: -65px
.alert-danger
	color: #a94442;
	background-color: #f2dede;
	padding: 10px;
	border: 1px solid #ebccd1;
	border-radius: 4px;
	font-size: 23px;
	text-align: center
	width 100%
	max-width 480px
	margin 30px auto 0
.pre_ht
	margin 5px 0 0
	display: inline-block
	background-color: #0c0c0c
	padding 5px 10px
	border: 2px solid
#home-task
	#header
		padding: 25px 0
		margin-bottom: 0
	#main
		text-align: center
		min-height 1px
		height auto
.ht_main_date
	margin-bottom: 10px