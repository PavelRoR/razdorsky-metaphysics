@media screen and (min-height 860px)
    #main
        min-height calc(100vh - 190px)
@media screen and (min-height 1400px) 
    #main
        min-height calc(100vh - 190px)
    .main_title
        margin 40px 0
    .main_date
        margin-bottom: 40px
    .video_wrapper_main
            max-width 850px
@media screen and (min-width 1900px) 
    #main
        min-height calc(100vh - 190px)
    .main_title
        margin 40px 0
    .main_date
        margin-bottom: 40px
    .video_wrapper_main
        max-width 850px
@media screen and (min-width 2290px)
    .container
        width 1400px
    #logo
        width auto
    .contacts a .fa+span
    .contacts a .fa
    #footer #conditions, #footer p
        font-size: 18px
    .pre
        font-size: 22px
    .main_sub_title
        font-size: 26px
    .video_wrapper_main
        max-width 1050px
    #home-task #header
        margin-bottom: 60px
    .flip-clock-wrapper
        margin-bottom: 40px
    .ht_main_date
        margin-bottom: 20px
    .video_wrapper_ht
        margin-top: 40px
        max-width 750px
@media screen and (max-width 1199px)
    .contacts a .fa
        margin: 0 8px 0 14px
@media screen and (max-width 991px)
    #header
        padding 15px 0
        margin-bottom: 10px
    #logo
        display block
        float: none
        margin 0 auto 10px
    .contacts
        float: none
        text-align: center
        a
            display: inline-block
@media screen and (max-width 767px)
    #vk
        margin-bottom: 25px
@media screen and (max-width 426px)
    #main
        padding-bottom: 30px
        min-height calc(100vh - 177px)
    #header
        margin-bottom: 5px
    #logo
        width: 160px
    .contacts a .fa
    .contacts a .fa + span
        font-size: 13px
    .main_title
        font-size: 32px
    .main_date
        font-size: 24px
        margin-bottom: 10px
    #footer
        padding 20px
    #footer h5
        font-size: 20px
        line-height: 1.3
    .soc_buttons a
        margin 10px
    #footer #conditions, #footer p
        font-size: 12px
    #ht_feedback
        padding 30px 0
    .ht_feedback_title
        font-size: 24px
        margin-bottom: 30px
        span
            font-size: 28px
    .flip-clock-wrapper
        margin 15px auto
        max-width 260px
        font-size: 10px
    .flip-clock-wrapper ul li a div div.inn
        font-size: 27px
    .flip-clock-wrapper ul li
        line-height: 33px
    .flip-clock-wrapper ul
        height 33px
    .flip-clock-dot
        width: 5px;
        height: 5px;
        left: 1px;
    .flip-clock-wrapper ul li a div.up::after
        top 16px
    .flip-clock-dot.top
        top 22px
    .flip-clock-dot.bottom
        bottom: 33px
    .flip-clock-divider .flip-clock-label
        right: -43px;
    .flip-clock-divider.hours .flip-clock-label
        right -46px
    .flip-clock-divider.minutes .flip-clock-label, .flip-clock-divider.seconds .flip-clock-label
        right -49px
@media screen and (max-width 360px)
    .main_title
        font-size: 28px
        margin 10px 0
    .main_date
        font-size: 22px
    .flip-clock-wrapper
        margin 10px auto
    .ht_feedback_title span
        font-size: 26px
    .ht_feedback_title
        font-size: 22px
        line-height: 1.3