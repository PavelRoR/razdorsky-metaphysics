#prices
	padding 60px 0 0
.price_container
	border: 1px solid rgba(#fff,.3)
	background-color: #0C0C0C
	width 100%
	max-width 320px
	margin 0 auto
	box-shadow: 0 0 10px 5px rgba(#fff, .55)
.price_head
	padding 10px
	border-bottom: 1px solid rgba(#fff,.3)
	text-align: center
.price_head_title
	font-size: 18px
	font-family: "AkrobatRegular", sans-serif;
	margin: 0
	color rgba(#fff, .5)
	span
		color: #3d74da;
		display: block
		font-size: 24px;
		font-family: "AkrobatBold", sans-serif;
		text-transform: uppercase;
		margin-top: 0
.price_body
	padding 20px
.price_body_title
	color #fff
	margin-bottom: 15px
	font-size: 20px
	font-family: 'OpenSansBold', sans-serif
	text-align: center
.price_body_list
	li
		list-style: none;
		padding-left: 20px;
		background-image: url("../img/triangle.png");
		background-repeat: no-repeat;
		background-position: 0 5px;
		font-size: 16px;
		font-family: 'OpenSansRegular', sans-serif
		color: #fff;
		line-height: 1.444;
		margin-bottom: 15px
		span
			font-family: 'OpenSansBold', sans-serif
		&:last-child
			margin-bottom: 0
.price_body_more
	background-color: transparent
	width 100%
	max-width 80px
	font-size: 15px
	font-family: 'OpenSansLight',sans-serif
	margin 0 auto
	display block
	border-bottom: 1px dashed
	color rgba(#fff,.8)
.price_footer
	padding: 20px
	border-top: 1px solid rgba(#fff,.3)
	text-align: center
.price
	font-size: 26px
	font-family: 'AkrobatBold',sans-serif
	margin-bottom: 15px
	color #fff
	span
		display: block
.price_full
	span
		text-decoration: line-through
.price_sale
	color: #ff0000
.price_today 
	color: #00FF00
.price_footer_button
	margin 25px auto 0
.timer
	h4
		font-size: 36px
		text-align: center
		color #ffffff
		margin: 40px 0 20px
		font-family: 'AkrobatBlack',sans-serif
.alert-danger
	color: #a94442;
	background-color: #f2dede;
	padding: 10px;
	width 100%
	max-width 360px
	margin 40px auto 0
	border: 1px solid #ebccd1;
	border-radius: 4px;
	font-size: 24px;
	text-align: center
.flip-clock-wrapper
	width: 100%
.flip-clock-divider .flip-clock-label
	color #fff
.flip-clock-dot
	background-color: #0075DE
.flip-clock-wrapper ul li a div div.inn
	color: #fff
	background-color: #0075DE
	text-shadow: 2px 2px 2px rgba(0,0,0,.6);